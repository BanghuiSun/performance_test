            <stringProp name="BeanShellAssertion.query">if(!ResponseCode.equals(&quot;200&quot;)){
 Failure = true;
 FailureMessage=&quot;喵驾小程序，首页列表接口 失败，*********返回码:&quot; + ResponseCode + &quot;\n&quot;;
 String response = prev.getResponseDataAsString();
 log.info(FailureMessage);
 log.info(&quot;===========================================================&quot;);
 log.info(response);
 return;
}

String response = prev.getResponseDataAsString();
Failure = !(response.contains(&quot;\&quot;code\&quot;:0&quot;));
if (Failure) {
 Failure = true;
 FailureMessage=&quot;喵驾小程序，首页列表接口未发现 \&quot;code\&quot;:0 &quot; + &quot;*********当前响应为：&quot; + response + &quot;\n&quot;;
